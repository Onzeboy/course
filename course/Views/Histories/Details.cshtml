@model course.Models.History

@{
    ViewData["Title"] = "Details";
}

<h1>Подробнее</h1>

<div>
    <h4>История заказов</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Пользователь
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.employer)
        </dd>
        <dt class = "col-sm-2">
            Колличество товара
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.quantity)
        </dd>
        <dt class = "col-sm-2">
            Цена
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.price)
        </dd>
        <dt class = "col-sm-2">
            Дата заказа
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.orderdate)
        </dd>
        <dt class = "col-sm-2">
           Город
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.adresscity)
        </dd>
        <dt class = "col-sm-2">
            Улица
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.adressstreet)
        </dd>
        <dt class = "col-sm-2">
           Дом
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.adresshome)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Вернуться к списку</a>
</div>
